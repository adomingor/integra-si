<html>
    <head>
        {% block meta %}
            <meta charset='utf-8'>
            <meta name='viewport' content='width=device-width, initial-scale=1'>
        {% endblock %}
        <title>{% block title %}INTEGRA - S. I.{% endblock %}</title>
        {% block stylesheets %}
          <link href='{{ asset("css/bootstrap.min.css") }}' type='text/css' rel='stylesheet'/>
          <link href='{{ asset("css/normalize.css") }}' type='text/css' rel='stylesheet'/>
          <link href='{{ asset("fonts/fontAwesome/css/font-awesome.min.css")}}' type='text/css' rel='stylesheet'/>
          <link href='{{ asset("css/integra.css")}}' type='text/css' rel='stylesheet'/>
          <link href='{{ asset("css/normalize.min.css")}}' type='text/css' rel='stylesheet'/>
          <link href='{{ asset("css/animate.css")}}' type='text/css' rel='stylesheet'/>
        {% endblock %}
        <link rel='icon' type='image/x-icon' href='{{ asset("favicon.ico") }}'/>
    </head>
    <body class='container-fluid'>
    {% block contenido%}
        <nav class="nav nav-inline">
            <a id="btn_menu" class="nav-link" href="#"><i class="fa fa-bars fa-2x" aria-hidden="true"></i></a>
            {% if is_granted("IS_AUTHENTICATED_FULLY") %}
                <!-- <a class='mdl-navigation__link' href='{{url("isi_sesion_homepage")}}'><i class='isi_icono_link_ctrado material-icons menu-icons'>dashboard</i>Sistema</a> -->
            {% else %}
                <a class='nav-link pull-xs-right' href='{{ url("login") }}'><i class="fa fa-sign-in fa-lg" aria-hidden="true"></i> Ingresar </a>
            {% endif %}
        </nav>
    {% endblock %}
    {% block javascripts %}
        <script src='{{ asset("js/jquery.js") }}' type='text/javascript'></script>
        <script src='{{ asset("js/bootstrap.min.js") }}' type='text/javascript'></script>
        <script src='{{ asset("js/integra.js") }}' type='text/javascript'></script>
    {% endblock %}
    </body>
</html>
