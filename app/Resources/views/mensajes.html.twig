        {% for flash in flashes %}
            <div class="mdl-color-text--{{label}}">
                <h6>{{ flash | raw }}</h6>
            </div>
        {% endfor %}
    {% endfor %}
    <span>
        <div id="isi_msjProcesando" class='mdl-spinner mdl-js-spinner is-active isi_quitarElemento'></div>
    </span>
</div>
